<p id="notice"><%= notice %></p>

<h1>Subsidios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Institucion</th>
      <th>Descripcion</th>
      <th>Modalidad</th>
      <th>Inicio</th>
      <th>Cierre</th>
      <th>Monto</th>
      <th>Perfil</th>
      <th>Bases</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subsidios.each do |subsidio| %>
      <tr>
        <td><%= subsidio.nombre %></td>
        <td><%= subsidio.institucion %></td>
        <td><%= subsidio.descripcion %></td>
        <td><%= subsidio.modalidad %></td>
        <td><%= subsidio.inicio %></td>
        <td><%= subsidio.cierre %></td>
        <td><%= subsidio.monto %></td>
        <td><%= subsidio.perfil %></td>
        <td><%= subsidio.bases %></td>
        <td><%= link_to 'Show', subsidio %></td>
        <td><%= link_to 'Edit', edit_subsidio_path(subsidio) %></td>
        <td><%= link_to 'Destroy', subsidio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subsidio', new_subsidio_path %>
